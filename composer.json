{
    "name": "eloquent/liberator",
    "description": "A proxy for circumventing PHP visibility keyword restrictions.",
    "keywords": [
        "visibility",
        "keyword",
        "access",
        "modifier",
        "object",
        "proxy",
        "private",
        "protected",
        "reflection"
    ],
    "homepage": "https://github.com/eloquent/liberator",
    "license": "MIT",
    "authors": [
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        }
    ],
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "errors/exceptions": "dev-master",
        "friendsofphp/php-cs-fixer": "1.11.4",
        "peridot-php/leo": "^1",
        "peridot-php/peridot": "^1",
        "peridot-php/peridot-code-coverage-reporters": "^2"
    },
    "autoload": {
        "psr-4": {
            "Eloquent\\Liberator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eloquent\\Liberator\\Test\\": "test/src"
        },
        "classmap": ["test/src"]
    }
}
